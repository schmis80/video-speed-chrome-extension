{
  "name": "Video Speed Controller",
  "version": "1.0",
  "description": "Speed Controller for HTML5 Videos",
  "manifest_version": 2,
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": ["style.css"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ]
}
